Algoritmo for_while_dowhile
	// INICIO GENERAL
	// DESARROLLO GENERAL
	Para variable<-valor_inicial Hasta valor_final Hacer
		// INICIO
		// DESARROLLO
		// INGRESO
		Mientras expresion_logica Hacer
			// INICIO
			// PROCESO
			Repetir
				// INICIO
				// INGRESO
				// PROCESO
				// PREGUNTA
			Hasta Que expresion_logica
			// FIN DO WHILE
			// INGRESO
		Fin Mientras
		// FIN WHILE
	FinPara
	// FIN GENERAL
FinAlgoritmo
